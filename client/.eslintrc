{
  "extends": ["wesbos"],
  "rules": {
    "react/prop-types": 0,
    "no-nested-ternary": 0,
    "jsx-a11y/control-has-associated-label": 0,
    "no-case-declarations": 0,
    "react/jsx-props-no-spreading": 0,
    "import/no-extraneous-dependencies": 0,
    "react/button-has-type": 0,
    "no-shadow": 0,
    "react/default-props-match-prop-types": 0,
    "no-alert": 0,
    "prettier/prettier": [
      "error",
      {
        "trailingComma": "none",
        "singleQuote": true,
        "printWidth": 120,
        "tabWidth": 2
      }
    ]
  }
}
